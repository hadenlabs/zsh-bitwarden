{
  "name": "zsh-bitwarden",
  "version": "0.0.0",
  "description": "bitwarden with zsh",
  "repository": "ssh://git@github.com/luismayta/zsh-bitwarden.git",
  "author": "@slovacus <slovacus@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist .next out",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.jsx,.mjs,.d.ts,.ts,.tsx ./.",
    "lint:fix": "eslint -c ./.eslintrc.js --fix . --ext .js,.jsx,.mjs,.d.ts,.ts,.tsx ./.",
    "prettier:fix": "prettier '**/*.{js?(on),ts?(x),scss,md,yml,yaml}' --write --list-different",
    "prettier": "prettier '**/*.{js?(on),ts?(x),scss,md,yml,yaml}' --write=false --list-different",
    "precommit": "lint-staged"
  },
  "publishConfig": {
    "access": "private"
  },
  "engines": {
    "node": ">=12"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "yarn precommit"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@equipindustry/eslint-config": "^0.2.0",
    "@equipindustry/prettierrc": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "husky": "^4.2.1",
    "rimraf": "^3.0.2",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.2"
  },
  "dependencies": {}
}
